{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "npm run build && wrangler pages dev",
    "postinstall": "nuxt prepare",
    "deploy": "npm run build && wrangler pages deploy",
    "db:bootstrap:local": "npx wrangler d1 execute preview-moma-gallery --local --file=./schema.sql",
    "db:bootstrap:preview": "npx wrangler d1 execute preview-moma-gallery --remote --file=./schema.sql",
    "db:validate": "npx wrangler d1 execute preview-moma-gallery --local --command='SELECT * FROM exhibits'",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@formkit/addons": "^1.6.2",
    "@formkit/nuxt": "^1.6.2",
    "@formkit/themes": "^1.6.2",
    "@heroicons/vue": "^2.1.3",
    "@nuxt/image": "^1.6.0",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "ai": "^3.0.34",
    "howler": "^2.2.4",
    "nuxt": "^3.11.2",
    "openai": "^4.38.5",
    "uuid": "^9.0.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240423.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@types/howler": "^2.2.11",
    "daisyui": "^4.10.2",
    "nitro-cloudflare-dev": "^0.1.4",
    "wrangler": "^3.52.0"
  }
}
